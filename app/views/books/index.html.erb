<h2>Books</h2>
<table>
  <thead>
    <th>Title</th>
    <th>Body</th>
    <th></th>
  </thead>
  <tbody></tbody>
  
</table>



<h2>New book</h2>

<%= form_with model: @book, url:'/todolists', local:true do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %><br>
  
  <%= f.submit 'Create Book' %>
<% end %>